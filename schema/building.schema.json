{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Building",
  "type": "object",
  "required": ["buildings"],
  "properties": {
    "buildings": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "key", "name"],
        "properties": {
          "id": { "type": "integer" },
          "key": { "type": "string" },
          "name": { "type": "string" },
          "category": { "type": "string" }
        }
      }
    }
  }
}