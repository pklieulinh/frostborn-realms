{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Doctrine",
  "type": "object",
  "required": ["doctrines"],
  "properties": {
    "doctrines": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "key", "name"],
        "properties": {
          "id": { "type": "integer" },
          "key": { "type": "string" },
          "name": { "type": "string" },
          "effects": { "type": "array", "items": { "type": "string" } }
        }
      }
    }
  }
}